    "bundle": "node_modules/.bin/webpack",
    "test": "node_modules/.bin/mocha --compilers js:babel-register src/**/*.spec.js",
    "make": "yarn run lint && yarn run clean && yarn run build && yarn run bundle && yarn run test"
  },
  "dependencies": {
